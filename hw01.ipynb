    "\n",
    "# calculate inverse\n",
    "A_inv = np.linalg.inv(A)\n",
    "B_inv = np.linalg.inv(B)\n",
    "C_inv = np.linalg.inv(C)\n",
    "\n",
    "# multiplying matrix\n",
